{
  "name": "cart-home-page",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "cart/home-page/src",
  "projectType": "library",
  "generators": {},
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "cart/home-page/src/**/*.ts",
          "cart/home-page/src/**/*.html"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "cart/home-page/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": ["{workspaceRoot}/coverage/cart/home-page"]
    }
  },
  "tags": ["scope:cart", "type:feature"]
}
