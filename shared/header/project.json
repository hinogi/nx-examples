{
  "name": "shared-header",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "shared/header/src",
  "projectType": "library",
  "generators": {},
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "shared/header/src/**/*.ts",
          "shared/header/src/**/*.html"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "shared/header/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": ["{workspaceRoot}/coverage/shared/header"]
    }
  },
  "tags": ["scope:shared", "type:ui"]
}
