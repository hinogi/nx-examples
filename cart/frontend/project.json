{
  "name": "cart",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "cart/frontend/src",
  "projectType": "application",
  "generators": {},
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "options": {
        "outputPath": "dist/cart/frontend",
        "webpackConfig": "cart/frontend/webpack.config.js",
        "index": "cart/frontend/src/index.html",
        "main": "cart/frontend/src/main.tsx",
        "polyfills": "cart/frontend/src/polyfills.ts",
        "tsConfig": "cart/frontend/tsconfig.app.json",
        "assets": [
          "cart/frontend/src/_redirects",
          {
            "input": "shared/assets/src/assets",
            "glob": "**/*",
            "output": "assets"
          },
          {
            "input": "shared/assets/src",
            "glob": "favicon.ico",
            "output": ""
          }
        ],
        "maxWorkers": 8,
        "styles": [
          "shared/styles/src/index.scss",
          "shared/header/index.scss",
          "node_modules/normalize.css/normalize.css"
        ],
        "scripts": [],
        "buildLibsFromSource": true,
        "isolatedConfig": true
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "cart/frontend/src/environments/environment.ts",
              "with": "cart/frontend/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "extractCss": true,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            }
          ]
        },
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        }
      },
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "options": {
        "buildTarget": "cart:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "cart:build:production"
        },
        "development": {
          "buildTarget": "cart:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["cart/frontend/**/*.{ts,tsx,js,jsx}"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "cart/frontend/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": ["{workspaceRoot}/coverage/cart/frontend"]
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "npx ts-node --project tools/tsconfig.tools.json tools/scripts/deploy --siteName nrwl-nx-examples-cart --outputPath dist/cart/frontend"
          }
        ]
      }
    }
  },
  "tags": ["type:app", "scope:cart"],
  "implicitDependencies": ["shared-assets", "shared-styles"]
}
