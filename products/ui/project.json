{
  "name": "products-ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "products/ui/src",
  "projectType": "library",
  "generators": {},
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "products/ui/src/**/*.ts",
          "products/ui/src/**/*.html"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "products/ui/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": ["{workspaceRoot}/coverage/products/ui"]
    }
  },
  "tags": ["scope:shared", "type:ui"]
}
